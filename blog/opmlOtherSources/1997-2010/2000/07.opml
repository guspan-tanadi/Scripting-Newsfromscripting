				<outline text="&lt;rules&gt;" created="Sat, 29 Jul 2000 05:02:46 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;textStyle&gt;bold&lt;/textStyle&gt;"/>
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backIssues/2000/07/29&lt;/internalLink&gt;"/>
						<outline text="&lt;/rule&gt; "/>
						</outline>
					<outline text="&lt;/rules&gt;"/>
