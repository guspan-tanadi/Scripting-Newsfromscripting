				<outline text="&lt;rules&gt;" created="Tue, 11 Nov 2003 05:02:46 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://blogs.law.harvard.edu/scriptingArchive/2003/11/11&lt;/internalLink&gt;"/>
