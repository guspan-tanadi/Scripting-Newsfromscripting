				<outline text="&lt;rules&gt;" created="Sat, 28 Sep 2002 05:02:58 GMT">
					<outline text="&lt;rule level=&quot;1&quot; to=&quot;1&quot;&gt;">
						<outline text="&lt;internalLink&gt;http://scriptingnews.userland.com/backissues/2002/09/28&lt;/internalLink&gt;"/>
