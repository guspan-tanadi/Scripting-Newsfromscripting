											<outline text="&lt;font face=&quot;geneva,arial&quot; size=-2&gt;">
												<outline text="&lt;center&gt;">
													<outline text="&lt;img src=&quot;http://www.scripting.com/gifs/mrsmithgoestowashington.gif&quot; height=143 width=180&gt;"/>
